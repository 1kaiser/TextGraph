{
  "name": "textgraph-exact",
  "version": "1.0.0",
  "description": "Exact implementation of Distill's text-as-graph visualization",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html --no-hmr",
    "dev:gat": "parcel gat-simple.html --no-hmr --host 0.0.0.0 --port 42040",
    "build": "parcel build index.html --public-url ./",
    "build:gat": "parcel build gat-explorer.html --public-url ./",
    "test": "node test/visual-comparison.js",
    "test:parallel": "node test/parallel-visual-test.js"
  },
  "keywords": [
    "graph",
    "visualization",
    "distill",
    "text-as-graph"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "d3": "^5.16.0",
    "d3-jetpack": "^2.2.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.55.0",
    "pngjs": "^7.0.0",
    "typescript": "^4.9.5"
  }
}
